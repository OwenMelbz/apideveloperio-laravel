{
    "name": "lezhnev74/http-analyzer-laravel-adapter",
    "description": "Laravel API adapter to track each http request app handled",
    "type": "library",
    "tags": [
        "http",
        "performance",
        "analytics",
        "logging",
        "request",
        "response"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Lezhnev",
            "email": "lezhnev.work@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.3.*",
        "guzzlehttp/guzzle": "~6.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "HttpAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HttpAnalyzerTest\\": "tests/"
        }
    }
}
